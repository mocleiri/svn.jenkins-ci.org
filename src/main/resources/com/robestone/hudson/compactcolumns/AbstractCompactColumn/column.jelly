<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler">
	<td data="${it.getColumnSortData(job)}">
		<j:choose>
		<j:when test="${!it.isBuildsEmpty(job)}">
      		<j:forEach var="build" items="${it.getBuilds(job)}">
				<j:if test="${!build.first}">
					<st:nbsp/>&gt;<st:nbsp/>
				</j:if>
				<a href="${jobBaseUrl}${job.shortUrl}${build.urlPart}" 
					style="color: ${build.color}; font-weight: ${build.fontWeight}"
					tooltip="&lt;b&gt;&lt;u&gt;${%Build} #${build.run.number}${build.getLatestBuildString(request.locale)}&lt;/u&gt;&lt;/b&gt;
						&lt;ul&gt;
						&lt;li&gt;${build.getBuiltAt(request.locale)}&lt;/b&gt;&lt;/li&gt;
						&lt;li&gt;${build.getStartedAgo(request.locale)}&lt;/li&gt;
						&lt;li&gt;${build.getLastedDuration(request.locale)}&lt;/li&gt;
						&lt;li&gt;&lt;b&gt;${build.status}&lt;/li&gt;
						&lt;/ul&gt;"
				>${build.timeAgoString}</a>
			</j:forEach>
		</j:when>
		<j:otherwise>
			${%N/A}
		</j:otherwise>
		</j:choose>
	</td>
</j:jelly>