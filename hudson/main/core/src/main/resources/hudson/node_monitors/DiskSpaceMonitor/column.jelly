<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:s="/lib/form">
  <j:set var="space" value="${from.getFreeSpace(c)}"/>
  <j:choose>
    <j:when test="${space==null}">
      <td data="-1">N/A</td>
    </j:when>
    <j:otherwise>
      <td data="${space}">${from.toHtml(space)}</td>
    </j:otherwise>
  </j:choose>
</j:jelly>
