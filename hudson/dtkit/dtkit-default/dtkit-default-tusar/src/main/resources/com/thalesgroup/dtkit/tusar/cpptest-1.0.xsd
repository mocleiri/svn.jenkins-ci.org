<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="ResultsSession">
    <xs:annotation>
      <xs:documentation> Root of the result session, contains xml of a single Parasoft tool
        execution. </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Authors">
          <xs:annotation>
            <xs:documentation>List of authors</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="VersionInfos"/>
        <xs:element ref="Locations"/>
        <xs:element ref="Scope"/>
        <xs:element ref="CodingStandards"/>
        <xs:element ref="Metrics"/>
        <xs:element ref="FunctionalTests"/>
        <xs:element ref="Generation"/>
        <xs:element ref="Exec"/>
        <xs:element ref="ExecutedTestsDetails"/>
        <xs:element ref="Goals"/>
        <xs:element ref="CodeReview"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.ResultsSession"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.ResultsSession">
    <xs:attribute name="time" use="required"/>
    <xs:attribute name="toolName" use="required"/>
    <xs:attribute name="toolVer" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="Authors">
    <xs:annotation>
      <xs:documentation>List of authors</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Author"/>
      </xs:sequence>      
    </xs:complexType>
  </xs:element>  
  <xs:element name="VersionInfos">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="StorageInfo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Scope">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ProjectInformations"/>
        <xs:element minOccurs="0" ref="TestingProcessProblems"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CodingStandards">
    <xs:annotation>
      <xs:documentation>Contains results of a static analysis run</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Goal"/>
        <xs:element ref="Projects"/>
        <xs:element ref="StdViols"/>
        <xs:element ref="Supps"/>
        <xs:element ref="History"/>
        <xs:element ref="Rules"/>
        <xs:element ref="FailedRuns"/>
        <xs:element ref="TestingProcessProblems"/>
        <xs:element ref="TestedFilesDetails"/>
        <xs:element name="FADetails"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.CodingStandards"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.CodingStandards">
    <xs:attribute name="ownerId" use="required"/>
    <xs:attribute name="time" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="Metrics">
    <xs:annotation>
      <xs:documentation>Contains results of a metrics run</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Metric"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.Metrics"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Metrics">
    <xs:attribute name="ownerId" use="required"/>
    <xs:attribute name="time" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="visibleDepth" use="required"/>
    <xs:attribute name="invSymbols" type="xs:boolean"/>
  </xs:attributeGroup>
  <xs:element name="FunctionalTests">
    <xs:annotation>
      <xs:documentation>Contains results of a functional test suite execution</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Goals"/>
        <xs:element ref="History"/>
        <xs:element ref="FuncViols"/>
        <xs:element ref="Categories"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.FunctionalTests"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.FunctionalTests">
    <xs:attribute name="ownerId" use="required"/>
    <xs:attribute name="time" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="Generation">
    <xs:annotation>
      <xs:documentation>Contains results of an auto test generation run</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>        
        <xs:element minOccurs="0" ref="TestingProcessProblems"/>
        <xs:element name="Projects">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" ref="GenerationProjectInfo"/>
            </xs:sequence>
          </xs:complexType>          
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="GenerationInformations"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.Generation"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Generation">
    <xs:attribute name="time" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="Exec">
    <xs:annotation>
      <xs:documentation>Contains results of unit tests execution</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Goals"/>
        <xs:element ref="ExecViols"/>
        <xs:element ref="History"/>
        <xs:element ref="Categories"/>
        <xs:element ref="Summary"/>
        <xs:element ref="Coverage"/>
        <xs:element ref="TestingProcessProblems">          
        </xs:element>
      </xs:choice>
      <xs:attributeGroup ref="attlist.Exec"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Exec">
    <xs:attribute name="ownerId" use="required"/>
    <xs:attribute name="time" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="CodeReview">
    <xs:annotation>
      <xs:documentation>Contains results of code reviews scanning session</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="TestingProcessProblems"/>
        <xs:element ref="ScannerReviews"/>
        <xs:element ref="UndefinedAuthors"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.CodeReview"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.CodeReview">
    <xs:attribute name="ownerId" use="required"/>
    <xs:attribute name="time" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="ScannerReviews">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Review"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Review">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Reviewers"/>
        <xs:element ref="Monitors"/>
        <xs:element ref="Sources"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.Review"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Review">
    <xs:attribute name="active" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="author" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="date" use="required"/>
    <xs:attribute name="new" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="num" use="required"/>
  </xs:attributeGroup>
  <xs:element name="Reviewers">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Reviewer"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Sources">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Source"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Reviewer">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Reviewer"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Reviewer">
    <xs:attribute name="active" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="name" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="status" use="required"/>
  </xs:attributeGroup>
  <xs:element name="Source">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Source"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Source">
    <xs:attribute name="issues" use="required"/>
    <xs:attribute name="path" use="required"/>
  </xs:attributeGroup>
  <xs:element name="Monitor">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Monitor"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Monitor">
    <xs:attribute name="active" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="name" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="status" use="required"/>
  </xs:attributeGroup>
  <xs:element name="StorageInfo">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.StorageInfo"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.StorageInfo">
    <xs:attribute name="ownerId" use="required"/>
    <xs:attribute name="resultId" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="ver" use="required"/>
  </xs:attributeGroup>
  <xs:element name="ProjectInformations">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="ScopeProjectInfo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FailedRuns">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="Src"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="StdViols">
    <xs:annotation>
      <xs:documentation>Collection of static analysis violations - bug detective, duplication,
        static analysis and metrics</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="DupViol"/>
        <xs:element ref="FlowViol">
          <xs:annotation>
            <xs:documentation>Bug Detective violation</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="MetViol">
          <xs:annotation>
            <xs:documentation>Metric violation</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="StdViol">
          <xs:annotation>
            <xs:documentation>Static Analysis violation</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="Supps">
    <xs:annotation>
      <xs:documentation>collection of suppressions</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Supp"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TestedFilesDetails">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Total"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Rules">
    <xs:annotation>
      <xs:documentation>contains information about rules, rule categories and
        severities</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="RulesList"/>
        <xs:element ref="CategoriesList"/>
        <xs:element ref="SeverityList"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Metric">
    <xs:annotation>
      <xs:documentation>contains information about calculated metric</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Info"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.Metric"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Metric">
    <xs:attribute name="aggType" use="required">
      <xs:annotation>
        <xs:documentation>Metric aggregation type, MAX (children maximum value used) or SUM
          (children values are added).</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="MAX"/>
          <xs:enumeration value="SUM"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="id" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="name" use="required">
      <xs:annotation>
        <xs:documentation>Metric name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="short" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="lang" use="required" type="xs:string"/>
  </xs:attributeGroup>
  <xs:element name="FuncViols">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="FuncViol">          
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GenerationInformations">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="GenerationInfo"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ExecViols">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="ExecViol">
          <xs:annotation>
            <xs:documentation>violation that is result of unit test execution</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="InsureViol">
          <xs:annotation>
            <xs:documentation>violation detected by Runtime Error Detection</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="InsureViol">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ElDescList"/>
      </xs:sequence>
      <xs:attribute name="auth" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>owner of the violation</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="cat" use="required" type="xs:integer">
        <xs:annotation>
          <xs:documentation>identifier of category</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="errTypeId" use="required" type="xs:string"/>
      <xs:attribute name="ftype" use="required">
        <xs:annotation>
          <xs:documentation>full name of type flagged with this violation</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="goals"/>
      <xs:attribute name="hash" use="required" type="xs:integer"/>
      <xs:attribute name="lang" use="required" type="xs:string"/>
      <xs:attribute name="ln" use="required" type="xs:integer">
        <xs:annotation>
          <xs:documentation>line number</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="locEndLn" type="xs:integer"/>
      <xs:attribute name="locEndPos" type="xs:integer"/>
      <xs:attribute name="locFile"/>
      <xs:attribute name="locStartPos" type="xs:integer"/>
      <xs:attribute name="locStartln" type="xs:integer"/>
      <xs:attribute name="locType" use="required" type="xs:string"/>
      <xs:attribute name="msg" use="required">
        <xs:annotation>
          <xs:documentation>error message</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pid" type="xs:integer"/>
      <xs:attribute name="rule" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>identifier of the runtime error detection rule</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="sev" use="required" type="xs:integer">
        <xs:annotation>
          <xs:documentation>severity of the rule</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="tool" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>parasoft tool</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="urgent" type="xs:boolean"/>
      <xs:attribute name="uri" type="xs:anyURI"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ElDesc">
    <xs:annotation>
      <xs:documentation>single element of execution path or stack trace</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Props"/>
        <xs:element ref="ElDescList"/>
      </xs:choice>
      <xs:attribute name="ElType" use="required" type="xs:string"/>
      <xs:attribute name="desc" use="required">
        <xs:annotation>
          <xs:documentation>description of element, usually describes point in source
            code</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="eln">
        <xs:annotation>
          <xs:documentation>end line in case of multi-line location</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rngLn"/>
      <xs:attribute name="sourceRngHash"/>
      <xs:attribute name="ln" type="xs:integer">
        <xs:annotation>
          <xs:documentation>line number</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="srcRngEndLn" type="xs:integer"/>
      <xs:attribute name="srcRngEndPos" type="xs:integer"/>
      <xs:attribute name="srcRngFile"/>
      <xs:attribute name="srcRngStartPos" type="xs:integer"/>
      <xs:attribute name="srcRngStartln" type="xs:integer"/>
      <xs:attribute name="srcRnghash" type="xs:integer"/>
      <xs:attribute name="srcRngLen" type="xs:integer"/>
      <xs:attribute name="srcRngOffs" type="xs:integer"/>
      <xs:attribute name="throwingMethod" type="xs:string"/>
      <xs:attribute name="thrownTypes" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Summary">
    <xs:annotation>
      <xs:documentation>execution statistics</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Total">
          <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
              <xs:element ref="CategoryErrors"/>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element ref="Projects"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Coverage">
    <xs:annotation>
      <xs:documentation>contains coverage information of executest unit tests</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Types"/>
        <xs:element ref="CvgStats"/>
        <xs:element ref="History"/>
        <xs:element ref="Files"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="ScopeProjectInfo">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="File"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.ScopeProjectInfo"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.ScopeProjectInfo">
    <xs:attribute name="fltFiles" use="required"/>
    <xs:attribute name="fltLns" use="required"/>
    <xs:attribute name="project" use="required"/>
    <xs:attribute name="totFiles" use="required"/>
    <xs:attribute name="totLns" use="required"/>
  </xs:attributeGroup>
  <xs:element name="DupViol">
    <xs:annotation>
      <xs:documentation>Duplication violation</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ElDescList"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.DupViol"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.DupViol">
    <xs:attribute name="NvActs" use="required"/>
    <xs:attribute name="NvType" use="required"/>
    <xs:attribute name="auth" use="required" type="xs:string">
      <xs:annotation>
        <xs:documentation>owner of violation</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="cat" use="required" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>category of violation</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="goals"/>
    <xs:attribute name="hash" use="required"/>
    <xs:attribute name="lang" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="ln" use="required">
      <xs:annotation>
        <xs:documentation>line number</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="locEndLn" use="required"/>
    <xs:attribute name="locEndPos" use="required"/>
    <xs:attribute name="locFile" use="required"/>
    <xs:attribute name="locStartPos" use="required"/>
    <xs:attribute name="locStartln" use="required"/>
    <xs:attribute name="locType" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="msg" use="required">
      <xs:annotation>
        <xs:documentation>error message</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rule" use="required" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>identifier of the duplication rule</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sev" use="required">
      <xs:annotation>
        <xs:documentation>severity of the rule</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="5"/>
          <xs:enumeration value="4"/>
          <xs:enumeration value="3"/>
          <xs:enumeration value="2"/>
          <xs:enumeration value="1"/>
          <xs:enumeration value="-1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="supp" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>is the violation supressed</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="urgent" type="xs:boolean"/>
    <xs:attribute name="tool" use="required" type="xs:string">
      <xs:annotation>
        <xs:documentation>parasoft tool</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="FlowViol">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Props"/>
        <xs:element ref="ElDescList"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.FlowViol"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.FlowViol">
    <xs:attribute name="FirstElSrcRngEndLn"/>
    <xs:attribute name="FirstElSrcRngEndPos"/>
    <xs:attribute name="FirstElSrcRngFile" use="required"/>
    <xs:attribute name="FirstElSrcRngLen"/>
    <xs:attribute name="FirstElSrcRngOffs"/>
    <xs:attribute name="FirstElSrcRngStartPos"/>
