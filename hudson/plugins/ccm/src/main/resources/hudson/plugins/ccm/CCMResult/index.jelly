<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler"
	xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson"
	xmlns:f="/lib/form" xmlns:i="jelly:fmt">
	<l:layout norefresh="true">
		<st:include it="${it.owner}" page="sidepanel.jelly" />
		<l:main-panel>

			<h1>${%CCMResults}</h1>

	        <p>${%AverageComplexity} <b>${it.report.averageComplexityPerMethod}</b></p>
	        <p>${%NumberOfMethods} <b>${it.report.numberOfMethods}</b></p>
	        <p>${%TotalComplexity} <b>${it.report.totalComplexity}</b></p>

		</l:main-panel>
	</l:layout>
</j:jelly>
