<FindBugsFilter>
  <Match>
      <Bug code="CD" />
      <Class name="~.*" />
  </Match>
  <Match>
      <Class name="~.Messages" />
  </Match>
  <Match>
      <Bug code="UwF, SIC, NP" />
      <Class name="~.*Test.*" />
  </Match>
  <Match>
      <Bug code="UwF" />
      <Class name="~.*View.*" />
  </Match>
</FindBugsFilter>
